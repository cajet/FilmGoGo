        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.administrator.filmgogoAdmin.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddShowMovie"/>
        <activity android:name=".Manager_add_votemovies"/>
        <activity android:name=".ShowTimeActivity"/>
        <activity android:name=".AddShowTime"/>
    </application>

</manifest>